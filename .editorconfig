root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.cs]
csharp_new_line_before_open_brace = methods,types,control_blocks,properties,accessors
dotnet_sort_system_directives_first = true
